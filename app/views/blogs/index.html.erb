<section class="blog">
      <h2 class="blog-headline">Recent Blogs</h2>
      <div class="blog-box">
        <% @blogs.each do |blog| %>
          <div class="blog-item clearfix">
            <div class="blog-image"><%= image_tag('img/forest.jpg',  class: 'blog-image') %></div>
            <section class="blog-text">
              <h3 class="blog-text-headline"><%= link_to blog.title, blog_path(blog) %></h3>
              <p class="blog-date"><%= blog.created_at %></p>
              <span class="category"><%= blog.category %></span>
            </section>
          </div>
        <% end %>
      </div>
      <div class="pagination">
        <p><<</p>
        <p>< </p>
        <p>1 </p>
        <p>2 </p>
        <p>3 </p>
        <p>> </p>
        <p>>> </p>
      </div>
    </section>